{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "description": "A C&I Studio cloud storage solution.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Undercover-Geek/qubee.git"
  },
  "keywords": [
    "ci-studios",
    "cloud-storage",
    "qubee"
  ],
