{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "description": "A C&I Studio cloud storage solution.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Undercover-Geek/qubee.git"
  },
  "keywords": [
    "ci-studios",
    "cloud-storage",
    "qubee"
  ],
  "author": "C&I Studios",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Undercover-Geek/qubee/issues"
  },
  "homepage": "https://qubee.io/",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "backend": "npm run server --prefix backend",
    "dev": "concurrently \"npm start\" \"npm run backend\"",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
    "minify-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css --source-map true"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-brands-svg-icons": "^5.13.1",
    "@fortawesome/free-regular-svg-icons": "^5.13.1",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@hapi/joi": "^17.1.1",
    "@sentry/browser": "^5.18.1",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "chart.js": "2.9.3",
    "classnames": "2.2.6",
    "dotenv": "^8.2.0",
    "filepond": "^4.18.0",
    "filepond-plugin-file-encode": "^2.1.6",
    "filepond-plugin-file-metadata": "^1.0.6",
    "filepond-plugin-file-rename": "^1.1.4",
    "filepond-plugin-file-validate-size": "^2.2.1",
    "filepond-plugin-file-validate-type": "^1.2.5",
    "filepond-plugin-image-exif-orientation": "^1.0.9",
    "filepond-plugin-image-resize": "^2.0.7",
    "lodash": "^4.17.15",
    "moment": "2.27.0",
    "node-sass": "^4.14.1",
    "nouislider": "14.5.0",
    "react": "16.13.1",
    "react-chartjs-2": "2.9.0",
    "react-copy-to-clipboard": "5.0.2",
    "react-datetime": "2.16.3",
    "react-dom": "16.13.1",
    "react-filepond": "^7.0.1",
    "react-google-maps": "9.4.5",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "react-toastify": "^6.0.6",
    "reactstrap": "8.5.1"
  },
  "devDependencies": {
    "@types/googlemaps": "3.39.8",
    "@types/markerclustererplus": "2.1.33",
    "@types/react": "16.9.41",
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4",
    "typescript": "^3.9.5"
  }
}
